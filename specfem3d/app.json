{
  "id": "specfem3d",
  "owner": "${apiUserId}",
  "version": "1.0",
  "description": "Containerized SPECFEM3D jupyter nb",
  "jobType": "BATCH",
  "runtime": "SINGULARITY",
  "runtimeOptions": ["SINGULARITY_RUN"],
  "containerImage": "docker://jthet/specfem3d:latest",
  "strictFileInputs": false,
  "jobAttributes": {
    "description": "SPECFEM3D Job Attributes",
    "execSystemId": "Frontera.exec",
    "dynamicExecSystem": false,
    "execSystemLogicalQueue": null,
    "isMpi": false,
    "parameterSet": {
      "appArgs": [],
      "containerArgs": [],
      "schedulerOptions": [],
      "envVariables": [
        {
          "key": "TERM",
          "value": "xterm-256color"
        }
      ],
      "archiveFilter": {
        "includes": [],
        "excludes": [],
        "includeLaunchFiles": false
      }
    },
    "fileInputs": [],
    "fileInputArrays": [],
    "memoryMB": 4000,
    "nodeCount": 4,
    "coresPerNode": 1,
    "maxMinutes": 45
  }
}